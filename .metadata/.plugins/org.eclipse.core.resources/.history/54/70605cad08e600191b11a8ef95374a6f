package br.edu.ifpb.gugawag.so.sockets;

public class Subscritor {
	private String ip;
	private int porta;
	
	
	public Subscritor(String ip, int porta) {
		this.ip = ip;
		this.porta = porta;
	}
	public String getIp() {
		return ip;
	}
	public void setIp(String ip) {
		this.ip = ip;
	}
	public int getPorta() {
		return porta;
	}
	public void setPorta(int porta) {
		this.porta = porta;
	}

}
